# Production Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit .env file to git!

# Database
DB_ROOT_PASSWORD=changeme_root_password
DB_USERNAME=rtb4free
DB_PASSWORD=changeme_db_password
DB_NAME=rtb4free

# Redis
REDIS_PASSWORD=changeme_redis_password

# Elasticsearch
ELASTIC_PASSWORD=changeme_elastic_password
ELASTICSEARCH_ENABLE=true

# Rails
RAILS_MASTER_KEY=  # Generate with: rails credentials:edit
SECRET_KEY_BASE=   # Generate with: rails secret

# Application
CUSTOMER_NAME=RTB4FREE
BIDDER_TIME_OFFSET=0
WEB_PORT=3000

# RTB Crosstalk
RTB_CROSSTALK_PORT=8100
RTB_CROSSTALK_USER=your_user
RTB_CROSSTALK_PASSWORD=your_password

# AWS S3
AWS_ACCESS_KEY_S3=your_access_key
AWS_SECRET_KEY_S3=your_secret_key
AWS_REGION_S3=us-east-1
AWS_S3_BUCKET=your-bucket-name

# Timezone
RTB_REPORT_TIMEZONE=America/Los_Angeles

# Docker
DOCKER_IMAGE=rtb4free/campaign-manager:latest
